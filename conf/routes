GET   /assets/*file                         controllers.Assets.at(path="/public", file)
GET   /webjars/*file                        controllers.WebJarAssets.at(file)

GET   /                                     controllers.HomeController.index
GET   /externaldatasource/:id               controllers.ExternalDataSourcesController.show(id: com.opendataclub.models.ExternalDataSourceId)
POST  /externaldatasource/:id/dataimports   controllers.DataImportsController.create(id: com.opendataclub.models.ExternalDataSourceId)
